  "dependencies": {
    "eslint": "^1.10.3",
    "history": "^1.17.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.0",
    "ava": "^0.9.1",
    "babel": "^6.3.13",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-jest": "^6.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.9.1",
    "browserify": "^13.0.0",
    "cssnano": "^3.0.0",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
